{
    "configurations": [
        {
            "name": "libopencm3-stm32f4",
            "includePath": [
                "${workspaceFolder}/pqm4/libopencm3/include",
                "${workspaceFolder}/pqm4/common",
                "${workspaceFolder}/pqm4/mupq/common",
                "${workspaceFolder}/src/saber/m4",
                "${workspaceFolder}/src/saber/m4_masking"
            ],
            "defines": [
                "STM32F4"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "c11",
